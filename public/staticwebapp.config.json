{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.map",
      "/*.ico",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
